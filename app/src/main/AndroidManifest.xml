<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e.englishquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconsquestion"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconsquestion"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizActivity"
            android:label="@string/quiz_page">
        </activity>
        <activity android:name=".CheatActivity"
            android:label="@string/cheat_page">
        </activity>
        <activity android:name=".AboutActivity"
            android:label="@string/about_page">
        </activity>
    </application>

</manifest>